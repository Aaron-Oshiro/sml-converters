{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "dbtToSml",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/dev.js",
      "args": ["dbt-to-sml", "-s ./../../jaffle-shop"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "smlToCortex",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/dev.js",
      "args": ["sml-to-cortex",  "-s path-to-sml-directory", "-o path-to-output-directory"],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "bimToSml",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/dev.js",
      "args": ["bim-to-sml", "-s path-to-bim-file", "-o path-to-output-directory"],
    }
  ]
}
